{
  "name": "@w3block/w3block-ui-sdk",
  "private": false,
  "version": "2.0.25",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./dist/style2.css": "./dist/style.css",
    "./auth": {
      "types": "./dist/auth.d.ts",
      "import": "./dist/auth.js",
      "default": "./dist/auth.js"
    },
    "./addFunds": {
      "types": "./dist/addFunds.d.ts",
      "import": "./dist/addFunds.js",
      "default": "./dist/addFunds.js"
    },
    "./affiliates": {
      "types": "./dist/affiliates.d.ts",
      "import": "./dist/affiliates.js",
      "default": "./dist/affiliates.js"
    },
    "./business": {
      "types": "./dist/business.d.ts",
      "import": "./dist/business.js",
      "default": "./dist/business.js"
    },
    "./shared": {
      "types": "./dist/shared.d.ts",
      "import": "./dist/shared.js",
      "default": "./dist/shared.js"
    },
    "./pass": {
      "types": "./dist/pass.d.ts",
      "import": "./dist/pass.js",
      "default": "./dist/pass.js"
    },
    "./custom": {
      "types": "./dist/custom.d.ts",
      "import": "./dist/custom.js",
      "default": "./dist/custom.js"
    },
    "./storefront": {
      "types": "./dist/storefront.d.ts",
      "import": "./dist/storefront.js",
      "default": "./dist/storefront.js"
    },
    "./tokens": {
      "types": "./dist/tokens.d.ts",
      "import": "./dist/tokens.js",
      "default": "./dist/tokens.js"
    },
    "./dashboard": {
      "types": "./dist/dashboard.d.ts",
      "import": "./dist/dashboard.js",
      "default": "./dist/dashboard.js"
    },
    "./checkout": {
      "types": "./dist/checkout.d.ts",
      "import": "./dist/checkout.js",
      "default": "./dist/checkout.js"
    },
    "./notifications": {
      "types": "./dist/notifications.d.ts",
      "import": "./dist/notifications.js",
      "default": "./dist/notifications.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "check:build": "yarn build",
    "dev": "yarn watch:webpack",
    "dev:watch": "concurrently \"yarn watch:webpack\" \"yarn watch:types\" \"yarn watch:css\" \"yarn watch:assets\"",
    "dev:verbose": "webpack --mode development --watch --progress --stats=verbose",
    "watch:webpack": "webpack --mode development --watch",
    "watch:types": "tsc --watch --preserveWatchOutput --emitDeclarationOnly",
    "watch:css": "chokidar \"src/**/*.css\" -c \"tailwindcss -i ./src/style.css -o ./dist/style.css\"",
    "watch:assets": "chokidar \"src/assets/**/*\" \"src/**/assets/**/*\" \"src/**/*.json\" -c \"yarn copy-assets && yarn copy-json\"",
    "prepare": "husky install",
    "prebuild": "yarn clean",
    "copy-assets": "copyfiles -u 1 \"src/**/assets/**/*\" dist/",
    "copy-json": "copyfiles -u 1 \"src/**/locales/**/*\" dist/ && copyfiles -u 1 \"src/**/*.json\" dist/",
    "build": "yarn copy-assets && yarn copy-json && tailwindcss -i ./src/style.css -o ./dist/style.css --minify && webpack && tsc --emitDeclarationOnly",
    "build:webpack": "webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "release": "standard-version",
    "pre-release": "standard-version --prerelease alpha",
    "setup-dev": "powershell -ExecutionPolicy Bypass -File scripts/link-dependencies.ps1"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@mertasan/tailwindcss-variables": "^2.7.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/eslint-plugin-query": "4",
    "@types/node": "^22.15.14",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/validator": "^13.15.1",
    "autoprefixer": "^10.4.7",
    "chokidar": "^4.0.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.9.2",
    "next": "15.2.2",
    "next-auth": "^4.24.11",
    "postcss": "^8.4.14",
    "postcss-loader": "^8.1.1",
    "standard-version": "^9.5.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.1.6",
    "tailwindcss-autofill": "^1.0.2",
    "tailwindcss-text-fill": "^0.2.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "url-loader": "^4.1.1",
    "vite-plugin-svgr": "^4.3.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.8.0",
    "@headlessui/react": "^1.7.17",
    "@hookform/resolvers": "^5.0.1",
    "@metamask/detect-provider": "^2.0.0",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "@szhsin/react-menu": "^4.4.0",
    "@tanstack/react-query": "^4.36.1",
    "@types/lodash": "^4.17.18",
    "@w3block/sdk-id": "^0.1.88",
    "axios": "^1.9.0",
    "card-validator": "^10.0.2",
    "classnames": "^2.5.1",
