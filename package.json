    "cpf-cnpj-validator": "^1.0.3",
    "ethers": "5.4.6",
    "html5-qrcode": "^2.3.8",
    "husky": "^9.1.7",
    "i18next": "^25.0.1",
    "i18next-browser-languagedetector": "^8.0.5",
    "js-file-download": "^0.4.12",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "openpgp": "^6.1.1",
    "qrcode.react": "^4.2.0",
    "react-credit-cards-2": "^1.0.3",
    "react-currency-mask": "^1.3.3",
    "react-dropzone": "^14.3.8",
    "react-google-autocomplete": "^2.7.5",
    "react-hook-form": "^7.56.3",
    "react-i18next": "^11.18.1",
    "react-imask": "^7.6.1",
    "react-phone-number-input": "^3.4.12",
    "react-tiny-popover": "^8.1.6",
    "react-use": "^17.6.0",
    "react-webcam": "^7.2.0",
    "rimraf": "^5.0.10",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.2.1",
    "swiper": "^11.2.6",
    "validator": "^13.15.15",
    "yup": "^1.6.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.36.1",
    "next": "15.2.2",
    "next-auth": "^4.24.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
